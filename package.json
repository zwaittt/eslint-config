{
  "name": "@urzx/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint ./ --fix",
    "dev": "tsup src/index.ts --format esm,cjs --dts-resolve --watch",
    "build": "tsup src/index.ts --format esm,cjs --minify --dts-resolve",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "prepublishOnly": "pnpm ts-check && pnpm build",
    "prepare": "pnpm build"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "dist"
  ],
  "typesVersions": {
    ">=3.2": {
      "*.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*.js"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16.20.0"
  },
  "dependencies": {
    "@eslint/js": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.23.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/eslint": "^8.44.6",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.8.10",
    "eslint": "8.26.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "eslint": "8.26.0 - 9"
  }
}
